{
  "expo": {
    "name": "News App",
    "slug": "NewsApp",
    "scheme": "newsapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.jpg",
    "userInterfaceStyle": "light",
    "android": {
      "package": "com.news.app",
      "versionCode": 6,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.jpg",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
        [
          "expo-splash-screen",
          {
            "image": "./assets/icon.jpg",
            "imageWidth": 47
          }
        ]
    ],
    "extra": {
      "router": {
        "origin": false
      }
    }
  }
}